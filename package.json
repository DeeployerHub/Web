{
    "name": "deeployer-web",
    "version": "1.0.0",
    "description": "A platform for online startups to find good teammates, manage the startup, help to get fund by VC or crowd sources. server & test platform",
    "main": "boot.js",
    "scripts": {
        "setup": "bower install --allow-root && npm install && gulp",
        "dev-run-supervisor": "npm test && supervisor -i public,upload,assets,node_modules,bower_components boot.js",
        "dev-run": "npm test && node boot.js",
        "start": "npm test && npm run dev-run",
        "test": "jshint . && exit 0"
    },
    "pre-commit": {
        "run": "test",
        "silent": true
    },
    "pre-push": {
        "run": "test",
        "silent": true
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/WAAC/d-web.git"
    },
    "contributors": [
