    },
    "homepage": "https://github.com/WAAC/d-web#readme",
    "dependencies": {
        "aws-sdk": "^2.2.43",
        "body-parser": "~1.13.2",
        "bower": "^1.7.7",
        "busboy": "^0.2.12",
        "connect-redis": "^3.0.2",
        "cookie-parser": "~1.3.5",
        "debug": "~2.2.0",
        "del": "^2.2.0",
        "express": "^4.13.4",
        "express-session": "^1.13.0",
        "file-stream-rotator": "0.0.6",
        "gulp": "^3.9.1",
        "gulp-sass": "^2.2.0",
        "gulp-sourcemaps": "^1.6.0",
        "mongoose": "^4.4.6",
        "morgan": "~1.6.1",
        "node-uuid": "^1.4.7",
        "passport": ">= 0.0.0",
        "passport-google-oauth": "^1.0.0",
        "passport-google-oauth2": ">= 0.0.0",
        "redis": "^2.4.2",
        "serve-favicon": "~2.3.0",
        "swig": "^1.4.2"
    },
    "devDependencies": {
